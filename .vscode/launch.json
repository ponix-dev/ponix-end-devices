{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "temp-humidity-rp2350",
            "cwd": "${workspaceFolder}/crates/temp-humidity-rp2350",
            "chip": "RP2350",
            "flashingConfig": {
                "flashingEnabled": true
            },
            "coreConfigs": [
                {
                    "programBinary": "${workspaceFolder}/target/thumbv8m.main-none-eabihf/release/temp-humidity-rp2350",
                    "rttEnabled": true,
                    "rttChannelFormats": [
                        {
                            "channelNumber": 0,
                            "dataFormat": "Defmt",
                            "showTimestamps": true
                        }
                    ]
                }
            ]
        },
